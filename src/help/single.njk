---
layout: default
pagination:
  data: helps
  size: 1
  alias: post
  addAllPagesToCollections: true
permalink: '/help/{{ post.slug }}/'
---

<section class="md:max-w-[65ch] mx-4 md:mx-auto">
  <h2 class="text-3xl md:text-4xl mb-0 font-bold text-primary-500">{{ post._embedded["wp:term"][0][0].name }}</h2>
	<h1 class="text-3xl md:text-4xl font-light">{{ post.title.rendered | safe }}</h1>

  <article class="prose nodark:prose-invert mt-10">
    {{ post.content.rendered | wpUrls | safe }}
  </article>

  <div class="flex flex-row flex-wrap md:flex-nowrap justify-start mt-8 text-primary-500">
			<a href="/help/" class="btn-outline rounded-full">
				<span class="mt-px whitespace-nowrap">Back to support</span>
			</a>
      {# <a href="https://app.defiplaza.net/swap/" class="btn-primary rounded-full ml-4 inline-flex items-center">
				<span class="mt-px whitespace-nowrap">Launch app</span>
				{% include "partials/arrow-right.njk" %}
			</a> #}
	</div>
</section>

